{
    "stock_dao":{
        "class":"dao.StockDao"
    },
    "rank":{
        "class":"data.rank.Rank"
    },
    "stock_db":{
        "class":"stdata.StockDbData",
        "properties":{
            "dao":{"bean":"stock_dao"},
            "rank":{"bean":"rank"}
        }
    },
    "price_data":{
        "class":"data.price.PriceData",
        "properties":{
            "comp_dao":{"bean":"comp_dao"},
            "price_dao":{"bean":"price_dao"}
        }
    },
    "comp_dao":{
        "class":"qi.database.CompanyDao",
        "init_args":[
            {"bean":"db"}
        ]
    },
    "price_dao":{
        "class":"qi.database.PriceDao",
        "init_args":[
            {"bean":"db"}
        ]
    },
    "erboard_dao":{
        "class":"qi.database.ERBoardDao",
        "init_args":[
            {"bean":"db"}
        ]
    },
    "er_data":{
        "class":"data.erdata.ERData",
        "properties":{
            "er_dao":{"bean":"erboard_dao"},
            "calc":{"bean":"calc"},
            "comp_dao":{"bean":"comp_dao"}
        }
    },
    "db":{
        "class":"qi.database.Database"
    },
    "calc":{
        "class":"data.calc.Calc",
        "properties":{
            "price_data":{"bean":"price_data"}
        }
    },
    "factory":{
        "class":"qi.database.DBFactory",
        "properties":{
            "conf_path":"conf/database.ini"
        }
    }
}